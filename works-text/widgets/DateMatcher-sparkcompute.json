{
  "metadata": {"spec-version": "1.5"},

  "display-name": "Date Matcher",
  "configuration-groups": [
    {
      "label": "Plugin Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Reference Name",
          "name": "referenceName"
        }
      ]
    },
    {
      "label": "Basic Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Text Field",
          "name": "textCol"
        },
        {
          "widget-type": "textbox",
          "label": "Date Field",
          "name": "outputCol"
        },
        {
          "widget-type": "textbox",
          "label": "Date Format",
          "name": "dateFormat",
          "widget-attributes": {
            "default": "yyyy/MM/dd"
          }
        },
        {
          "widget-type": "select",
          "label": "Output Option",
          "name": "outputOption",
          "widget-attributes": {
            "values": [
              "extract",
              "replace"
            ],
            "default": "replace"
          }
        }        

      ]
    }
  ]
}