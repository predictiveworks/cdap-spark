{
  "metadata": {"spec-version": "1.5"},

  "display-name": "Sent2Vec Embedding",

  "icon": {
    "arguments": {
      "data": ""
    },
    "type": "inline"
  },

  "configuration-groups": [
     {
       "label": "Plugin Configuration",
       "properties": [
         {
           "widget-type": "textbox",
           "label": "Reference Name",
           "name": "referenceName"
         }
       ]
     },
     {
      "label": "Model Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Model Name",
          "name": "modelName"
        },
        {
          "widget-type": "select",
          "label": "Model Stage",
          "name": "modelStage",
          "widget-attributes": {
            "values": [
              "experiment",
              "staging",
              "production",
              "archived"
            ],
            "default": "experiment"
          }
        }
      ]
     },
     {
       "label": "Data Configuration",
       "properties": [
         {
           "widget-type": "textbox",
           "label": "Text Field",
           "name": "textCol"
         },
         {
           "widget-type": "textbox",
           "label": "Sentence Field",
           "name": "sentenceCol"
         },
         {
           "widget-type": "textbox",
           "label": "Embedding Field",
           "name": "embeddingCol"
         }
       ]
     },
     {
       "label": "Parameter Configuration",
       "properties": [
         {
           "widget-type": "select",
           "label": "Normalization",
           "name": "normalization",
           "widget-attributes": {
             "values": [
               "false",
               "true"
             ],
             "default": "true"
           }
         },
         {
           "widget-type": "select",
           "label": "Pooling Strategy",
           "name": "poolingStrategy",
           "widget-attributes": {
             "values": [
               "average",
               "sum"
             ],
             "default": "average"
           }
         }
       ]
     }
  ]
}