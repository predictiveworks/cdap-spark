{
  "metadata": {"spec-version": "1.5"},

  "display-name": "Apply Rules",

  "icon": {
    "arguments": {
      "data": "data:image/png;base64,"
    },
    "type": "inline"
  },

  "configuration-groups": [
    {
      "label": "Plugin Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Reference Name",
          "name": "referenceName"
        }
      ]
    },
	{
	  "label": "Rule Configuration",
	  "properties": [
        {
          "widget-type": "textarea",
          "label": "Drools Ruleset",
          "name": "rules",
          "widget-attributes": {
            "rows": "10"
          }
        },
        {
          "widget-type": "select",
          "label": "Ruleset Type",
          "name": "rulesType",
          "widget-attributes": {
            "values": [
              "filter",
              "score"
            ],
            "default": "filter"
          }
        }
	  ]
	}
]
}