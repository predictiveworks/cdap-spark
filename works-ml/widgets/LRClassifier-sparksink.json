{
  "metadata": {"spec-version": "1.5"},

  "display-name": "LR Classifier",

  "icon": {
    "arguments": {
      "data": "data:image/png;base64,"
    },
    "type": "inline"
  },

  "configuration-groups": [
     {
       "label": "Plugin Configuration",
       "properties": [
         {
           "widget-type": "textbox",
           "label": "Reference Name",
           "name": "referenceName"
         }
       ]
     },
     {
      "label": "Model Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Model Name",
          "name": "modelName"
        },
        {
          "widget-type": "select",
          "label": "Model Stage",
          "name": "modelStage",
          "widget-attributes": {
            "values": [
              "experiment",
              "staging",
              "production",
              "archived"
            ],
            "default": "experiment"
          }
        }
      ]
    },
     {
       "label": "Data Configuration",
       "properties": [
         {
           "widget-type": "textbox",
           "label": "Features Field",
           "name": "featuresCol"
         },
         {
           "widget-type": "textbox",
           "label": "Label Field",
           "name": "labelCol"
         },
         {
           "widget-type": "textbox",
           "label": "Data Split",
           "name": "dataSplit",
           "widget-attributes": {
             "default": "70:30"
           }
         }
       ]
     },
    {
      "label": "Parameter Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Maximum Iterations",
          "name": "maxIter",
          "widget-attributes": {
            "default": "20"
          }
        },
        {
          "widget-type": "textbox",
          "label": "ElasticNet Mixing",
          "name": "elasticNetParam",
          "widget-attributes": {
            "default": "0.0"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Regularization Parameter",
          "name": "regParam",
          "widget-attributes": {
            "default": "0.0"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Conversion Tolerance",
          "name": "tol",
          "widget-attributes": {
            "default": "1E-6"
          }
        }
      ]
    }
  ]
}
